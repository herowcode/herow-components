{
  "name"                : "select-estado",
  "description"         : "Combobox para seleção de estados brasileiros com fetch dinâmico da API do IBGE",
  "version"             : "1.0.0",
  "registryDependencies": ["combobox", "input-group"],
  "dependencies"        : [],
  "devDependencies"     : [],
  "frameworks"          : {
    "nextjs": {
      "files"   : [
        {"name": "select-estado.tsx", "target": "components"                                  },
        {"name": "actions.ts",        "target": "actions",    "targetName": "select-estado.ts"}
      ],
      "withForm": {
        "files"               : [
          {"name": "select-estado-form.tsx", "target": "components", "targetName": "select-estado.tsx"}
        ],
        "dependencies"        : ["react-hook-form", "@hookform/resolvers", "zod"],
        "registryDependencies": ["form"]
      }
    },
    "vite"  : {
      "files"   : [ {"name": "select-estado.tsx", "target": "components"} ],
      "withForm": {
        "files"               : [ {"name": "select-estado-field.tsx", "target": "components"} ],
        "dependencies"        : ["react-hook-form", "@hookform/resolvers", "zod"],
        "registryDependencies": ["form"]
      }
    }
  }
}
